{
"name": "Task Tracker - React",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Lets you add a task tracker embedded inside a website that persists data to browser.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"]
    }
  ],
  "browser_action": {
    "default_popup": "./popup.html"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    "https://fonts.gstatic.com/s/materialicons/*"
  ]
}