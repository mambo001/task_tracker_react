{
  "name": "task_tracker_react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "material-icons": "^1.4.1",
    "materialize-css": "^1.0.0-rc.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-shadow": "^19.0.2",
    "react-shadow-root": "^6.1.0",
    "rewire": "^5.0.0",
    "styled-components": "^5.3.1",
    "use-persisted-state": "^0.3.3",
    "yarn": "^1.22.11"
